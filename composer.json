{
  "name": "orthanc-tower/contracts",
  "description": "Shared type-safe contracts for Orthanc Tower ecosystem",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "orthanc",
    "contracts",
    "dto",
    "enums",
    "laravel"
  ],
  "authors": [],
  "require": {
    "php": "^8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "larastan/larastan": "^2.9",
    "laravel/pint": "^1.13"
  },
  "autoload": {
    "psr-4": {
      "OrthancTower\\Contracts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OrthancTower\\Contracts\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit",
    "analyse": "vendor/bin/phpstan analyse src tests",
    "format": "vendor/bin/pint"
  }
}
